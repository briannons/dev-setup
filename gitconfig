[core]
    editor = vim
[merge]
    tool = vimdiff

[alias]
    br = branch
    brD = branch -D
    brm = branch -m
    cm = commit
    cma = commit -a
    cmaA = commit -a --amend
    cmf = commit --amend -C HEAD
    co = checkout
    cod = checkout develop
    com = checkout master
    cob = checkout -b
    cp = cherry-pick
    did = diff develop
    dih = diff HEAD
    dim = diff master
    logls = log --name-status
    logpr = log --graph --pretty=format:'%Cred%h%Creset -%C(bold green)%d%Creset %s (%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    logtr = log --graph --pretty='%C(auto)%h %s %C(cyan)-%an%C(auto)%+d %C(auto)%ci' --all
    logtrb = log --graph --format='format:%C(yellow)%h%C(reset) %C(blue)\"%an\" <%ae>%C(reset) %C(magenta)%ar%C(reset)%C(auto)%d%C(reset)%n%s' --date-order
    mg = merge
    mgs = merge --squash
    pl = pull
    pu = push
    puO = push --set-upstream origin HEAD
    rb = rebase
    rba = rebase --abort
    rbc = rebase --continue
    rbd = rebase develop
    rbm = rebase master
    rbs = rebase --skip
    st = status -s -b
    us = reset HEAD
